OnBombGoldFlower
{
	if QuickStartVersion{
		NeedUpdate
		return
	}
	_txt="";
	mode="-1"
	mess="";
	if BombGoldFlowerBoot==NULL
	{
		Card;
		usercoin=20000;
		_txt+=OnCardStory;
	}
	BombGoldFlowerBoot=1;
	playBombGoldFlower=1;
	running="playBombGoldFlower"
	cardmode=1;                 //开始模式
	deskmoney=0;                //当局全部赌注数额
	wager=50;                   //赌注大小
	cardturn=0;                 //说话顺序。
	banker=0;                   //庄家（0：橘花；2：用户；1；斗和）
	user.numcard=IARRAY;        //用户牌型
	towa.numcard=IARRAY;        //斗和牌型
	kikka.numcard=IARRAY;       //橘花牌型
	opentowa=0;                 //斗和开牌
	openkikka=0;                //橘花开牌
	towapass=0;                 //斗和弃牌
	kikkapass=0;                //橘花弃牌
	circle=0;                   //单局回合数
	if bombdayoff!=day
	{
		bombdayoff=day;
		kikkacoin=10000;
		towacoin=10000;
	}
	kikkaflowermoney=kikkacoin;
	towaflowermoney=towacoin;
	cardwin=-1;
	_txt+BombGoldFlowerInit()
}

