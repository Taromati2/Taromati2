CardAICheck
{
     _numcard=_argv[0]
     _k1=_argv[1]
     _k2=_argv[2]
     _k3=_argv[3]
     _k4=_argv[4]
     _fear=circle*0.25+wager*0.002

     if CardWeight("%(_numcard)",_k1,_k2,_k3,_k4,1)<0  //跟据判断决定是否弃牌
          {1;return}

     elseif CardWeight("%(_numcard)",_k1,_k2,_k3,_k4,_fear)>=0  //跟据判断决定是否跟注
          {0;return}
     else      //其他情况则开牌
          {2;return}
 
}

CardWeight
{
     _numcard=_argv[0]
     _k1=_argv[1]
     _k2=_argv[2]
     _k3=_argv[3]
     _k4=_argv[4]
     _k5=_argv[5]
     _weight=compare("%(_numcard)")*_k1+maxcard("%(_numcard)")*_k2+middlecard("%(_numcard)")*_k3+leastcard("%(_numcard)")*_k4-_k5
     _weight


}



ShowCard
{
     _txt=""
     cardX=90
     cardY=240
     _txt+="\_l[%(cardX),%(cardY-20)]%(username)"
     _txt+=winstring("%(user.numcard)")+" "+maxcard("%(user.numcard)")+DrawCard("%(user.numcard)")
     cardX=0
     cardY=116
     _txt+="\_l[%(cardX),%(cardY-20)]橘花"
     if openkikka!=1
          _txt+=DrawBk
     else
          _txt+=winstring("%(kikka.numcard)")+" "+maxcard("%(kikka.numcard)")+DrawCard("%(kikka.numcard)")
     cardX=180
     cardY=116
     _txt+="\_l[%(cardX),%(cardY-20)]斗和"
     if opentowa!=1
          _txt+=DrawBk
     else
          _txt+=winstring("%(towa.numcard)")+" "+maxcard("%(towa.numcard)")+DrawCard("%(towa.numcard)")

    _txt
}

