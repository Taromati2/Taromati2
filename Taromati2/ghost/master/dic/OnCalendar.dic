		{
			_buff=FREAD(_file)
			if _buff!=-1
			_temp+=_buff+"|";
		}
		FCLOSE(_file);
	}
	
	if _mode=="all"
	{
		_temp;
		return;
	}
	SETDELIM(_temp,'|')
