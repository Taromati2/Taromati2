			}	
		}

		when "remindmodeselect","soundpathsave","countmodeselect","apppathsave"
		{
			SETDELIM(eventtemp, "|");
			_temp=eventtemp[_id];
			_temp[_k,'	']=_t
			eventtemp[_id]=_temp
