OnOpenMenu
{
		click=null
	if reference0 != "" {
		OpenMenu(reference0,reference1);
	}
	else {
		OpenMenu;
	}
}



OpenMenu
{
		ResetVar;
	_submenu = 0;
		_submenuex=0;  
		_message="";
	if _argc > 0 {
		_submenu = _argv[0];
				_submenuex=_argv[1];
				menu=_submenu
				menuex=_submenuex
	}
	else {
		_message=OpenMenuTalk;
	}
	;
		fjswitch=language
	_sub = "◆,◇,◇,◇,◇,◆,◇,◇,◇,◇,◆,◇,◇,◇,◇,◆,◇,◇,◇,◇,◆";

	"\C\![quicksession,true]\1\b[-1]\c\0\c"+_message+"\b[2]\_l[0,28]\q[%(_sub[_submenu])常用功能,OnSubMenu,0,%(_submenuex)]/
