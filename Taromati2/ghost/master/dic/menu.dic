OpenMenuTalk
{
	_phase = GetPhase;
		if intimacy>1000 && _phase == 2 {
			click=TOINT(click)+1
			"\0\s[5]\b[2]%(username)%(Timejud)好，什么事呢"
		   if 亲密度>=200 
			"\0\s[5]\b[2]最喜欢%(username)了，什么事呢"
			"\0\s[26]\b[2]什么事呢、%(username)？"
			"\0\s[41]\b[2]啊…好的。"
	    "\0\s[5]\b[2]要做什么呢？\s[40]"
	    "\0\s[26]\b[2]啊…是的。"
		   if 腹黑度>=200
			"\0\s[27]\b[2]需要帮忙吗。"
		   if 萌度>=200
			"\0\s[22]\b[2]橘花待命。"
		   if 腹黑度>依赖度
			 "\0\s[32]碍事的东西通通见鬼去吧……"
		   elseif 腹黑度>=250
			 "\0\s[27]%(username)要让橘花消灭谁呢"
