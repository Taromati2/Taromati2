AITalk
{
	if RAND(100)<85-communicate[1]*20&&communicate[0]
		StartCommunicate
	else
	{
		if ISVAR("talkmemo") == 1
		{
			ERASEVAR("talkmemo")
		}
		if mikireflag == 0
		{
			if CHAIN.IDName == "" 
			{
				if smoking == "1"
				{
					lastTalk = SmokingEnd
				}
				elseif rest == "1"
				{
					lastTalk = RestEnd
				}
				else
				{
					lastTalk = RandomTalk
				}
			}
			else
			{
				lastTalk = ChainTalk
			}
		OnLastTalkToChatroom(lastTalk)
		--
	                lastTalk;

		}
		elseif mikireswitch == "1"
		{
			MikireTalk
		}
	}
}

OnTranslate
{ 
	_txt = reference0;
	_fjswitch=-1;
	if _argc>0
	{
		_txt=_argv[0];
		_fjswitch=_argv[1];
	}
        if fjswitch == 1||_fjswitch==1
	{   
   	_txt = FUNCTIONEX("saori\ChConverter.dll", "traditional", _txt) //繁体化     
   	}
        elseif fjswitch==2||fjswitch==3||_fjswitch==2||_fjswitch==3
         {
         _txt=TranslateWithDictionary(_txt)
         }
	_txt=REPLACE(_txt,'\![]','');
	_temp=0
	if running==""
		_temp=TOINT(voice)
	if _temp
	{
		_t=SakuraToTxt(_txt);
		--
		OnFileWrite(,_t);
		--
		if running!="chatroom"&&"引擎 (" !_in_ _t&&CheckProcess("wscript.exe")==0//&&running!="readbook"&&read!="stop"
		TxtToVoice(_t,voicename);
		--
	     	_txt;
	}
	else
    		_txt;
} 

OnLastTalkToChatroom
{
	_lasttalk=_argv[0];
	_txt=""
	if chatroom==1
	{
		_usertime="%(hour):%(minute):%(second)";
		_lastTalk=REPLACE(_lasttalk,'|','CHR(124)');
		//_lastTalk=RE_REPLACE(_lastTalk,'\\e.*','');
		_selftime="%(hour):%(minute):%(second)";
		_kerotime="%(hour):%(minute):%(second)";
		_kikkatalk=""
		_towatalk=""
		_kikkatxt=""
		_towatxt=""
		_lastTalk=SPLIT(_lastTalk,'\')
		_n=ARRAYSIZE(_lastTalk)
		for _i=1;_i<_n+1;_i++
		{
			_Talk='\'+_lastTalk[_i]
			_talk=0
			if _Talk=='\0'
			{
				_talk=1;
				_selftime="%(hour):%(minute):%(second)";
				chatroomtxt+="#|\n\0%selfname %(_selftime)\n";
			}
			elseif  _Talk=='\1'		
			{
	///RE_REPLACE("\0\s[20]11111111111111\1\s[19]ssssssss\0\s[22]2222222222\1\s[16]aaaaaaaaaa",'\\s\[\d*\]','');
				_talk=1
				_kerotime="%(hour):%(minute):%(second)";
				chatroomtxt+="#|\n\0%keroname %(_kerotime)\n";
			}
			else
			{
				_Talk=RE_REPLACE(_Talk,'\\e.*','');
				_Talk=RE_REPLACE(_Talk,'\\s\[\d*\]','');
				chatroomtxt+="\0%(_Talk)";
			}
			
		}
	//	chatroomtxt+="#";
	}
	//WriteChatroom;
}

