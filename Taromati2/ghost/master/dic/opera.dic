	_str=REPLACE(_str,'橘花：','\0');
	_str=REPLACE(_str,'斗和：','\1');
	_str=REPLACE(_str,"“","");
	_str=REPLACE(_str,"”","");
	_str=REPLACE(_str,'　','\w5');
	_str=REPLACE(_str,'、','、\w9');
	_str=REPLACE(_str,'，','，\w5\w5');
	_str=REPLACE(_str,'。','。\w9\w9');
	_str=REPLACE(_str,'！','！\w9\w9');
	_str=REPLACE(_str,'？','？\w9\w9\w9');
	_str=REPLACE(_str,'…','…\w9\w9');
	foreach _face;_i
	{
		_surface='\s['+_i[0,'|']+']';
		_name='（'+_i[1,'|']+'）';
		_str=REPLACE(_str,_name,_surface);
		
	}
	//_str=REPLACE(_str,"<","[");
	//_str=REPLACE(_str,">","]");
	_str;
}











