		_buff = FREAD(_file);
		while _buff != -1 && _testid !_in_ _buff  {
			_buff = FREAD(_file);
		}
	           if _testid _in_ _buff
                    {
                        _buff = FREAD(_file);
			while _buff != -1 && _buff != "" {
				_test += _buff + "\n";_
                          _buff = FREAD(_file);
			}
		}
	}
	else
	_test+="文件不存在，请确认文件%(_file)";
	FCLOSE(_file);
	_test;

}






