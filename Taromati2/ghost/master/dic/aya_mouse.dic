//---- OnMouseMoveイベント --------------------------------------------------------------
OnMouseMove
{

	if mode == "1"
	{
		OnMouseMove1
	}
	else
	{
		OnMouseMove0
	}
}

OnMouseMove0
{
	//---- reference3を见て、本体侧かkero侧かを判断する
	if reference3 == 0
	{
		if reference4 != ""
		{
			stroke++
			if stroke >= 32
			{
				if reference4 == "Head"
				{
					headpatflag = 1
					headpattime = 0
					_talk = "TalkSakuraHeadPat.%(GetPhase).%(headpatcount)"
					if ISFUNC(_talk) == 1
					{
						EVAL(_talk)
						headpatcount++
					}
					else
					{
						EVAL("TalkSakuraHeadPat.%(GetPhase)")
					}
				}
				elseif reference4 == "Face"
				{
					_talk = "TalkSakuraFacePat.%(GetPhase)"
					if ISFUNC(_talk) == 1
					{
						EVAL(_talk)
					}
					else
					{
						TalkSakuraFacePat
					}
				}
				elseif reference4 == "Bust"
				{
					bustpatflag = 1
					bustpattime = 0
					_talk = "TalkSakuraBustPat.%(GetPhase).%(bustpatcount)"
					if ISFUNC(_talk) == 1
					{
						EVAL(_talk)
						bustpatcount++
					}
					else
					{
						EVAL("TalkSakuraBustPat.%(GetPhase)")
					}
				}
				elseif reference4 == "Hand"
				{
					_talk = "TalkSakuraHandPat.%(GetPhase)"
					if ISFUNC(_talk) == 1
					{
						EVAL(_talk)
					}
					else
					{
						TalkSakuraHandPat
					}
				}
				stroke = 0
			}
		}
		else
		{
			stroke = 0
		}
	}
	else
	{
		stroke++
		if stroke >= 64
		{
			if reference4 == "Head"
			{
				TalkKeroHeadPat
			}
			elseif reference4 == "Tail"
			{
				TalkKeroTailPat
			}
			else
			{
//				TalkKeroPat
			}
			stroke = 0
		}
	}
}


