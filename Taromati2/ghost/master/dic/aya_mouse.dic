OnMouseDoubleClick
{
	if mode == "1"
	{
		OnMouseDoubleClick1
	}
	else
	{
		OnMouseDoubleClick0
	}
}



OnMouseDoubleClick0
{
	if reference3 == 0
	{
		if reference4 == "Head"
		{
			_talk = "TalkSakuraHeadClick.%(GetPhase)"
			if ISFUNC(_talk) == 1
			{
				EVAL(_talk)
			}
			else
			{
				TalkSakuraHeadClick
			}
		}
		elseif reference4 == "Face"
		{
			_talk = "TalkSakuraFaceClick.%(GetPhase)"
			if ISFUNC(_talk) == 1
			{
				EVAL(_talk)
			}
			else
			{
				TalkSakuraFaceClick
			}
		}
			if reference4 == "Bust"
			{
				bustclickflag = 1
				bustclicktime = 0
				_talk = "TalkSakuraBustClick.%(GetPhase).%(bustclickcount)"
				if ISFUNC(_talk) == 1
				{
					EVAL(_talk)
					bustclickcount++
				}
				else
				{
					EVAL("TalkSakuraBustClick.%(GetPhase)")
				}
			}
		elseif reference4 == "Hand"
		{
			_talk = "TalkSakuraHandClick.%(GetPhase)"
			if ISFUNC(_talk) == 1
			{
				EVAL(_talk)
			}
			else
			{
				TalkSakuraHandClick
			}
		}
		else
		{
            OpenMenu
		}
	}
	else
	{
		if reference4 == "Head"
		{
			TalkKeroHeadClick
		}
		elseif reference4 == "Tail"
		{
			TalkKeroTailClick
		}
		else
		{
			OnMusicPlayer;
		}
	}
}

